{
  "extends": [
    "next/core-web-vitals",
    "prettier",
    "plugin:import/recommended"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ],
        "map": [
          [
            "@",
            "./src"
          ]
        ]
      }
    }
  }
}